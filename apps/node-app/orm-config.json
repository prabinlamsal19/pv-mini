{
    "type": "postgres",
    "host": "node-db",
    "port": 5432,
    "username": "${POSTGRES_USER}",
    "password": "${POSTGRES_PASSWORD}",
    "database": "nodeDatabse",
    "synchronize": true,
    "logging": false,
    "entities": [ 
      "src/entity/**/*.ts"
    ],
    "migrations": [
      "src/migration/**/*.ts"
    ],
    "subscribers": [
      "src/subscriber/**/*.ts"
    ]
  }